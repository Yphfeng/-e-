/* pages/merchants/index/index.wxss */
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
}
.userinfo{
  font-size: 32rpx;
}
.head{
  padding: 20rpx 0;
  text-align: center;
}
.title{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 40rpx;
  margin-top: 40rpx;
}
.red{
  color: red
}
.list{
  margin: 40rpx -20rpx 0;
  width: 100%;
  
}
.list .item{
  width: 50%;
  float: left;
  padding: 0 20rpx;
  margin-bottom: 50rpx;
  box-sizing: border-box;
}
.item-1{
  background: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.unLogin{
  width: 40%;
  display: inline-block;
}
.footer{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
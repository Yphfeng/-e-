/* pages/channelProvider/sgoodsinfo/sgoodsinfo.wxss */
.mian{
  padding: 20rpx;
}
.head{
  font-size: 34rpx;
  display: flex;
  justify-content: space-between;
}
.datanum{
  line-height: 85rpx;
  font-weight: 500;
}
.datanum text{
  margin-left: 20rpx;
}
.btn{
  padding: 10rpx 60rpx;
  color: white;
  background: #169BD5;
  border-radius: 6rpx;
  display: inline-block;
  margin-bottom: 20rpx;
}

/* 列表 */
.list{
  border-top: 1rpx solid #ccc ;
}
.listinfo{
  padding: 10rpx;
  display: flex;
  justify-content: space-around;
  border-bottom: 1rpx dashed #ccc;
  height: 84rpx;
}
.listinfo>view{
  line-height: 35rpx;
  position: relative;
  flex: 1;
  padding-left: 20rpx;
  font-size: 27rpx;
}
.infol::before{
  position: absolute;
  content: "";
  background: #ccc;
  height: 60rpx;
  width: 1rpx;
  right: 20rpx;
  top: 15rpx;
}
/* 分页按钮 */
.page{
  position: fixed;
  display: flex;
  bottom: 10rpx;
  right: 10rpx;
  flex-direction:row;
  font-size: 30rpx;
}
.page view{
  background: white;
  padding: 5rpx 10rpx;
  margin-left: 5rpx;
}
.page .activepage{
  background: #9AC5FF;
}
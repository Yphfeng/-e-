/* boolSugarDataAnalysis.wxss */

page {
  box-sizing: border-box;
}
.topChooseView {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}

.dateChooseView {
  flex: 4;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.biaotiImageView {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  color: gray;
  font-size: 29rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  height: 58rpx;
}



.buttonView {
  background-color: #f45b4a;
  color: white;
  height: 30px;
  border-radius: 8rpx;
  width: 80px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 29rpx;
  border-width: 1rpx;
  border-style: solid;
  border-color: lightgray;
}

.text8 {
  text-align: center;
  line-height: 73rpx;
  font-size: 29rpx;
}

.dataView {
  background-color: white;
  height: 73rpx;
  width: 100%;
  border-top: 2rpx solid #e2e2e2;
  display: flex;
  flex-direction: row;
}

.view1 {
  flex: 0 0 25%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-right: 2rpx solid #e2e2e2;
}

.view2 {
  flex: auto;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.biaotImage {
  height: 73rpx;
  width: 100%;
}

.biaotImage .biaotiImageView {
  position: relative;
  top: -73rpx;
  height: 73rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding-left: 20rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
}

.biaotImage .biaotiImageView .biaotiTitle {
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  color: white;
}

.imageOuterView {
  margin-top: 27rpx;
  width: 100%;
  box-sizing: border-box;
}

.imageOuterView .imageContentView {
  position: relative;
  width: 100%;
  top: -10rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: flex-start;
}

.imageOuterView .imageContentView canvas {
  width: 100%;
  height: 200px;
}
